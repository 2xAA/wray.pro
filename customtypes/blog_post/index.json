{
  "id": "blog_post",
  "label": "Blog Post",
  "repeatable": true,
  "status": true,
  "format": "custom",
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Slug"
        }
      },
      "date": {
        "type": "Timestamp",
        "config": {
          "label": "Date",
          "placeholder": ""
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "single": "heading1",
          "label": "Title"
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "single": "paragraph,heading2,strong,em",
          "label": "description"
        }
      },
      "thumbnail": {
        "type": "Image",
        "config": {
          "constraint": {},
          "thumbnails": [],
          "label": "thumbnail"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "html_code": {
              "type": "SharedSlice"
            },
            "text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
